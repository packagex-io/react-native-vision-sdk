{
  "name": "react-native-vision-sdk",
  "version": "0.8.30",
  "description": "VisionSDK for React Native.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn install && yarn example pods",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "vision-sdk",
    "ocr",
    "barcode",
    "qr-code"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/packagex-io/react-native-vision-sdk.git"
  },
  "author": "PackageX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/packagex-io/react-native-vision-sdk/issues"
  },
  "homepage": "https://github.com/packagex-io/react-native-vision-sdk#readme",
  "contributors": [
    {
      "name": "Ali Khan",
      "email": "ali.khan@packagex.io",
      "url": "https://github.com/aikhan79"
    }
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@commitlint/config-conventional": "^17.7.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@release-it/conventional-changelog": "^7.0.2",
    "@types/jest": "^29.5.5",
    "@types/react": "~18.2.28",
    "@types/react-native": "0.72.3",
    "commitlint": "^17.7.2",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "pod-install": "^0.1.39",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-builder-bob": "^0.23.1",
    "release-it": "^16.2.1",
    "typescript": "^5.2.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "resolutions": {
    "@types/react": "18.2.28"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "packageManager": "yarn@1.22.21",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "dependencies": {
    "jest-cli": "^29.7.0",
    "jetifier": "^2.0.0"
  }
}
